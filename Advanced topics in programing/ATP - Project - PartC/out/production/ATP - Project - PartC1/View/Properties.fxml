<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="300.0" prefWidth="293.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.MyPropertiesController">
   <children>
      <VBox prefHeight="227.0" prefWidth="204.0">
         <children>
            <Label text="Select generate algorithm" />
            <ChoiceBox fx:id="box_generate" prefWidth="150.0" />
            <Button fx:id="SetGene" mnemonicParsing="false" onMouseClicked="#updateGenerateAlgo" text="Set" />
            <Label text="Select solve algorithm" />
            <ChoiceBox fx:id="box_solve" prefWidth="150.0" />
            <Button fx:id="SetSolv" mnemonicParsing="false" onMouseClicked="#updateSolveAlgo" text="Set" />
            <Label text="Select character" />
            <ChoiceBox fx:id="box_character" prefWidth="150.0" />
            <Button fx:id="SetChar" mnemonicParsing="false" onMouseClicked="#updateCharachter" text="Set" />
         </children>
      </VBox>
   </children>
</AnchorPane>
