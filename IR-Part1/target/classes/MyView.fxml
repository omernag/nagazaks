<BorderPane fx:id="mainPane" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="EngineUserInterface.Controller">
    <left>
        <VBox prefHeight="579.0" prefWidth="423.0" spacing="5.0" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
            <GridPane minWidth="-Infinity" prefHeight="335.0" prefWidth="413.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="132.0" minWidth="10.0" prefWidth="106.0" />
               <ColumnConstraints hgrow="SOMETIMES" maxWidth="84.0" minWidth="10.0" prefWidth="100.0" />
               <ColumnConstraints hgrow="SOMETIMES" maxWidth="84.0" minWidth="10.0" prefWidth="100.0" />
               <ColumnConstraints hgrow="SOMETIMES" maxWidth="84.0" minWidth="10.0" prefWidth="100.0" />
               <ColumnConstraints hgrow="SOMETIMES" maxWidth="84.0" minWidth="10.0" prefWidth="100.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES" />
                    <RowConstraints maxHeight="83.0" minHeight="10.0" prefHeight="29.0" vgrow="SOMETIMES" />
                    <RowConstraints maxHeight="88.0" minHeight="10.0" prefHeight="41.0" vgrow="SOMETIMES" />
                    <RowConstraints maxHeight="91.0" minHeight="10.0" prefHeight="43.0" vgrow="SOMETIMES" />
                    <RowConstraints maxHeight="64.0" minHeight="10.0" prefHeight="33.0" vgrow="SOMETIMES" />
                    <RowConstraints maxHeight="61.0" minHeight="10.0" prefHeight="61.0" vgrow="SOMETIMES" />
                    <RowConstraints />
                    <RowConstraints />
                    <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0" />
                    <RowConstraints maxHeight="19.0" minHeight="0.0" prefHeight="0.0" />
                </rowConstraints>
                <Button fx:id="btn_ConnectToEn" onAction="#pressConnect" prefHeight="39.0" prefWidth="234.0" text="Connect To Engine" GridPane.columnSpan="2" GridPane.rowIndex="2" />
                <Button fx:id="btn_loadDict" onAction="#pressLoad" prefHeight="39.0" prefWidth="237.0" text="Load Dictionary" GridPane.columnSpan="2" GridPane.rowIndex="3" />
                <Button fx:id="btn_displayDict" layoutX="10.0" layoutY="162.0" onAction="#pressDisplay" prefHeight="39.0" prefWidth="267.0" text="Display Dictionary" GridPane.columnSpan="2" GridPane.rowIndex="4" />
                <CheckBox fx:id="cbx_stemming" mnemonicParsing="false" onAction="#pressStemming" prefHeight="27.0" prefWidth="170.0" text="Stemming" GridPane.columnSpan="2" GridPane.rowIndex="6" />
                <Button fx:id="btn_clear" layoutX="10.0" layoutY="10.0" onAction="#pressClear" prefHeight="40.0" prefWidth="263.0" text="Clear The Posting" GridPane.columnSpan="2" GridPane.rowIndex="5" />
                <Label fx:id="lbl_status" prefHeight="27.0" prefWidth="351.0" text="Dictionary Status:" GridPane.columnSpan="4" GridPane.rowIndex="7" />
                <Label fx:id="tf_status" text="Waiting" GridPane.columnIndex="2" GridPane.rowIndex="7" />
                <Button fx:id="btn_Set" layoutX="10.0" layoutY="48.0" onAction="#pressSet" prefHeight="0.0" prefWidth="178.0" text="Set Path" GridPane.columnSpan="2" GridPane.rowIndex="1" />
                <Button fx:id="btn_search" minHeight="-Infinity" mnemonicParsing="false" onAction="#openSearchWindow" prefHeight="152.0" prefWidth="152.0" text="Search" GridPane.columnIndex="3" GridPane.columnSpan="2" GridPane.rowIndex="1" GridPane.rowSpan="3" />
